{
  "name": "ctk-goteborg",
  "version": "1.0.0",
  "description": "Project to show for interview at CTK",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "npm run jshint && npm run tests",
    "jshint": "jshint ./lib ./test",
    "test-spec": "tap test/*.js"
  },
  "devDependencies": {
    "jshint": "^2.8.0",
    "tap": "^1.4.1"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "jade": "^1.11.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "stylus": "0.42.3"
  },
  "jshintConfig": {
    "node": true,
    "browser": false,
    "curly": true,
    "eqeqeq": true,
    "nonbsp": true,
    "undef": true,
    "unused": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilhem/ctk-goteborg.git"
  },
  "keywords": [
    "ctk",
    "goteborg",
    "api",
    "web"
  ],
  "author": "Emil Hemdal <emil.hemdal.em@gmail.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/emilhem/ctk-goteborg/issues"
  },
  "homepage": "https://github.com/emilhem/ctk-goteborg#readme"
}